{
  "name": "tradingview-syariah-indicator",
  "version": "1.0.0",
  "description": "Trading view syariah indicator",
  "private": true,
  "scripts": {
    "start": "web-ext run --firefox=\"/Applications/Firefox.app/Contents/MacOS/firefox-bin\" --start-url https://my.tradingview.com/symbols/MYX-FRONTKN/",
    "start:chrome": "web-ext run --target chromium --start-url www.tradingview.com/screener",
    "prebuild": "rimraf ./web-ext-artifacts/",
    "build": "web-ext build --ignore-files ./update-data/index.js",
    "update-data": "node ./update-data"
  },
  "engines": {
    "node": "12.14.0",
    "npm": "6.13.4"
  },
  "keywords": [
    "Syariah Stock in Malaysia",
    "Malaysia",
    "Stock",
    "Bursa Malaysia"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.0",
    "web-ext": "^3.2.1",
    "webextension-polyfill": "^0.5.0"
  }
}
